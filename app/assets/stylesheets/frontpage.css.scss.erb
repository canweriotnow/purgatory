.front {
  width: 100%;
  height: 600px;
  display: block;
  background-image: url(<%= asset_path 'tortured.jpg' %>);
  background-attachment: fixed;
  background-repeat: no-repeat;
  background-position: top right;
}

.front h1 {
  font-family: 'IM Fell English', serif;
  font-size: 48pt;
  padding-top: 5px;
}

.front-body {
  width: 60%;
  background-color: transparent;
}
.front-body p {
  font-family: 'IM Fell English', serif;
}

#sample-report {
  width: 60%;
  margin-top: 1em;
  padding-top: 1em;
}

.sreport {
  h2 {
    font-family: 'IM Fell English', serif;
  }
}

.row-fluid {
  background-image: url(<%= asset_path 'sword.png' %>);
  background-repeat: no-repeat;
  background-position: bottom left;
}