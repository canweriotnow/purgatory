.front {
  width: 100%;
  height: 600px;
  display: block;
  background-image: url(<%= asset_path 'tortured.jpg' %>);
  background-attachment: fixed;
  background-repeat: no-repeat;
  background-position: top right;
}

.front h1 {
  font-family: 'IM Fell English', serif;
  font-size: 48pt;
  padding-top: 5px;
}

.front-body {
  width: 60%;
  background-color: transparent;
}
.front-body p {
  font-family: 'IM Fell English', serif;
}